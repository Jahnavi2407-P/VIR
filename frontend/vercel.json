{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend",
  "installCommand": "npm install || yarn install || pnpm install",
  "framework": null,
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  },
  "regions": ["auto"],
  "functions": {
    "api/*/.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
